!! Appearance
urxvt.termName: rxvt-unicode
urxvt.scrollBar: false
!! urxvt.background: black
!! urxvt.foreground: gray
!! Font prefferenes
!urxvt*font: xft:Anonymice\ Powerline:pixelsize=16:antialias=true:hinting=true,xft:Sauce Code\ Pro\ Nerd\ Font\ Complet:pixelsize=18:antialias=true:hinting=true
urxvt*font: xft:Anonymice\ Powerline:pixelsize=16:antialias=true:hinting=true,xft:SauceCodePro\ Nerd\ Font Mono:pixelsize=20:antialias=true:hinting=true
!!  urxvt*font: xft:Anonymice\ Powerline:pixelsize=16:antialias=true:hinting=true,xft:Sauce Code\ Pro\ Nerd\ Font\ Complet:pixelsize=18:antialias=true:hinting=true
!! URxvt*font: xft:Anonymice\ Powerline:pixelsize=16:antialias=true:hinting=true,xft:Anonymice\ Powerline:pixelsize=18:antialias=true:hinting=true
!! URxvt*font: xft:Sauce Code\ Pro\ Nerd\ Font\ Complet:pixelsize=16:antialias=true:hinting=true
!! urxvt*font: xft:Anonymice\ Powerline:pixelsize=16:antialias=true:hinting=true
!! URxvt*font: xft:Source\ Code\ Pro\ Medium:pixelsize=13:antialias=true:hinting=true,xft:Source\ Code\ Pro\ Medium:pixelsize=13:antialias=true:hinting=true
!! urxvt.boldFont: xft:DejaVu Sans Mono:pixelsize=13:weight=bold
!! urxvt.font: xft:DejaVu Sans Mono:pixelsize=13
!! URxvt*font: xft:Source\ Code\ Pro:pixelsize=13:antialias=true:hinting=true,xft:Source\ Code\ Pro:pixelsize=13:antialias=true:hinting=true
!! URxvt*font: xft:Sauce\ Code\ Pro\ Medium:pixelsize=13:antialias=true:hinting=true,xft:Sauce\ Code\ Pro\ Medium:pixelsize=13:antialias=true:hinting=true
!! urxvt*font: xft:Terminus:size=10,xft:Inconsolata\ for\ Powerline:size=10

!! urxvt.letterSpace: -1
!! Larger history limit
urxvt.saveLines: 30000

! do not scroll with output
URxvt*scrollTtyOutput: false

! scroll in relation to buffer (with mouse scroll or Shift+Page Up)
URxvt*scrollWithBuffer: true

! scroll back to the bottom on keypress
URxvt*scrollTtyKeypress: true

!! Perl extensions
!! urxvt.perl-ext-common: default,matcher,font-size,selection-to-clipboard,resize-font
urxvt.perl-ext-common: default,matcher,selection-to-clipboard,autocomplete-ALL-the-things
URxvt.keysym.M-slash:  perl:aAtt:complete

!! delimiters for double-click selection
URxvt*cutchars: '"'&()*,;<=>?@[]^{|}:
!! Open urls in browser with Control-Click
urxvt.urlLauncher: /usr/bin/brave
urxvt.matcher.button: C1
!! Change font size on Control-Plus/Minus
urxvt.keysym.Control-0x2b: perl:font-size:increase
urxvt.keysym.Control-0x2d: perl:font-size:decrease

!! Yeah, I am one of those, who use these keys in Vim :-(
urxvt.keysym.Home:          \033[1~
urxvt.keysym.End:           \033[4~
urxvt.keysym.Control-Up:    \033[1;5A
urxvt.keysym.Control-Down:  \033[1;5B
urxvt.keysym.Control-Left:  \033[1;5D
urxvt.keysym.Control-Right: \033[1;5C

!! Cursor
urxvt.cursorBlink:          true
urxvt.cursorColor:          #657b83
!! urxvt.cursorUnderline:      true

! Pointer
urxvt.pointerBlank:         true

! Test solarized yevgenko
!-------------------------------------------------------------------------------
! Xft settings
!-------------------------------------------------------------------------------

Xft.dpi:                    96
! Xft.antialias:              false
Xft.rgba:                   rgb
Xft.hinting:                true
Xft.hintstyle: hintslight

URxvt.depth:                32
#URxvt.transparent:          false
URxvt.transparent:          true
URxvt.fading:               20
URxvt.shading:              20

URxvt.fadeColor:            #2c2b20
URxvt.urgentOnBell:         true
URxvt.visualBell:           true
URxvt.loginShell:           true
URxvt.internalBorder:       3
URxvt.lineSpace:            0

urxvt*foreground: #cccccc
urxvt*background: #0d1b1d

*color0:  #3f3f3f
*color1:  #c09070 
*color2:  #60c45a 
*color3:  #dfaf8f 
*color4:  #506070 
*color5:  #dc8cc3 
*color6:  #8cd0d3 
*color7:  #dcdccc 
*color8:  #709080 
*color9:  #df5353 
*color10:  #56bf0f
*color11:  #f0df5f
*color12:  #648fc3
*color13:  #ec93f3
*color14:  #93e0e3
*color15:  #ffffff
